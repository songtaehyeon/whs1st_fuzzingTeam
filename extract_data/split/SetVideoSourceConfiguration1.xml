            <sch:Extension>
               <!--Optional:-->
               <sch:Rotate>
                  <!--type: RotateMode - enumeration: [OFF,ON,AUTO]-->
                  <sch:Mode>?</sch:Mode>
                  <!--Optional:-->
                  <!--type: int-->
                  <sch:Degree>?</sch:Degree>
                  <!--Optional:-->
                  <sch:Extension>
                     <!--You may enter ANY elements at this point-->
                  </sch:Extension>
               </sch:Rotate>
               <!--Optional:-->
               <sch:Extension>
                  <!--You may enter ANY elements at this point-->
