   <soap:Header/>
   <soap:Body>
      <wsdl:GetReplayUri>
         <wsdl:StreamSetup>
            <!--type: StreamType - enumeration: [RTP-Unicast,RTP-Multicast]-->
            <sch:Stream>?</sch:Stream>
            <sch:Transport>
               <!--type: TransportProtocol - enumeration: [UDP,TCP,RTSP,HTTP]-->
               <sch:Protocol>?</sch:Protocol>
               <!--Optional:-->
               <sch:Tunnel/>
            </sch:Transport>
            <!--You may enter ANY elements at this point-->
         </wsdl:StreamSetup>
         <!--type: ReferenceToken-->
         <wsdl:RecordingToken>?</wsdl:RecordingToken>
      </wsdl:GetReplayUri>
   </soap:Body>
</soap:Envelope>