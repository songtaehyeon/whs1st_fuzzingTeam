   <soap:Header/>
   <soap:Body>
      <wsdl:ContinuousMove>
         <!--type: ReferenceToken-->
         <wsdl:ProfileToken>?</wsdl:ProfileToken>
         <wsdl:Velocity>
            <!--Optional:-->
            <sch:PanTilt x="?" y="?" space="?"/>
